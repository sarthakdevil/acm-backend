//connecting prisma
generator client{
    provider = "prisma-client-js"
}

datasource db{
    provider = "mysql"
    url = env("Database_Url")
}
//creating user model
model User{
    Sno Int @id @default(autoincrement())
    username String @unique
    password String @unique
    createdAt  DateTime @default(now())
    updatedAt DateTime @updatedAt

}
